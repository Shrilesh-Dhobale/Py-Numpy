   "source": [
    "#slicing of 1-D array\n",
    "import numpy as np\n",
    "arr=np.array([1, 2, 3, 4, 5])\n",
    "print(arr[1:4])\n",
    "print(arr[-4:-1])\n",
    "print(arr[1:5:2])#slicing with steps\n",
    "print(arr[4:1:-2])#-ve indexing and -ve steps\n",
    "print(arr[:2])"
   ]
  }
